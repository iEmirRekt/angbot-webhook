                                        popup.querySelector('.swal2-html-container').style.color = '#fff';
                                        popup.querySelector('.swal2-popup').style.backgroundColor = '#070b1e';
                                    }
                                }).then((result) => {
                                    window.location.reload();
                                }).catch((err) => {
                                    console.error("Swal error:", err);
